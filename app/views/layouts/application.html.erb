<!DOCTYPE html>
<html>
  <head>
    <title>Evenote</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <%= render 'header' %>
    </header>
    <%= yield %>
    <footer>
      <%= render 'footer' %>
    </footer>
  </body>
</html>
